{
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "target": "es2022",
    "lib": ["es2023"],
    "module": "Node16",
    "declaration": true,
    "esModuleInterop": true,
    "moduleResolution": "Node16",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "wasm-src",
    "outDir": "dist"
  },
  "include": ["wasm-src/**/*.ts"]
}
